<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.esg.mapper.TrMapleMapper">

   <insert id="TrMapleinsert">
	
		insert into trLoaBoard (userid,subject,content,ip,file)
		values(#{userid},#{subject},#{content},#{ip},#{file})
   </insert>
           
   <select id="TrMapleList" resultType="trMapleVO">
	
		select * from trMapleboard <!-- 대소문자확인 -->
	
   </select>
	
	
</mapper>